<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../resources/MainTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="groupFive.TeknikerController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <ButtonBar fx:id="buttonBar" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <buttons>
                  <Label />
                  <Label fx:id="labelArrTitle" alignment="TOP_CENTER" contentDisplay="CENTER" text="Tekniker" />
                <Button fx:id="goHome" mnemonicParsing="false" onAction="#goBack" prefWidth="407.0" text="Logg ut" />
              </buttons>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <padding>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </padding>
            </ButtonBar>
         </top>
         <left>
            <VBox fx:id="vboxBorderLeft" prefWidth="335.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="vboxSearch">
                     <children>
                        <Label fx:id="labelFestival" alignment="CENTER" contentDisplay="TOP" text="Festival">
                           <font>
                              <Font size="18.0" />
                           </font>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                        <Label text="Søk blant teknikere">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </Label>
                        <TextField fx:id="textFieldSearch" alignment="CENTER" maxWidth="210.0" onKeyReleased="#onKeyPressSearchBar" prefHeight="26.0" prefWidth="200.0" promptText="Søk..">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </TextField>
                     </children>
                  </VBox>
                  <ScrollPane fx:id="scrollPane">
                    <content>
                      <AnchorPane fx:id="vboxLightTech">
                           <children>
                              <VBox fx:id="vBoxTekniker" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <padding>
                                    <Insets left="5.0" />
                                 </padding>
                              </VBox>
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </AnchorPane>
                    </content>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ScrollPane>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </left>
         <center>
            <VBox fx:id="borderCenter" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="labelNavn" alignment="TOP_CENTER" contentDisplay="TOP" text="Trykk på en teknikker for å vise arbeidsinformasjon">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="30.0" left="255.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefWidth="200.0">
                     <children>
                        <VBox>
                           <children>
                              <Label text="Dato">
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ListView fx:id="listViewDato" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="150.0">
                           <children>
                              <Label text="Artist">
                                 <padding>
                                    <Insets bottom="15.0" />
                                 </padding>
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <ListView fx:id="listViewArtist" prefHeight="501.0" prefWidth="200.0" />
                           </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="150.0">
                           <children>
                              <Label text="Scene">
                                 <padding>
                                    <Insets bottom="15.0" />
                                 </padding>
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <ListView fx:id="listViewScene" prefHeight="501.0" prefWidth="200.0" />
                           </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="150.0">
                           <children>
                              <Label text="Oppgave">
                                 <padding>
                                    <Insets bottom="15.0" />
                                 </padding>
                                 <font>
                                    <Font name="Arial Bold" size="13.0" />
                                 </font>
                              </Label>
                              <ListView fx:id="listViewOppgave" prefHeight="501.0" prefWidth="200.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
